{
    "presets": [
        "env",   
        "react", 
        "stage-0"
    ],
    "plugins": [
        [
            "transform-runtime",  
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true
            }
        ],
        "transform-decorators-legacy",
        [  
            "import",         
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ]
    ]
}
